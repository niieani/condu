    "webpack": "^5.101.0",
    "webpack-cli": "^6.0.1",
    "webpack-merge": "^6.0.1"
  },
  "condu": {
    "managedDependencies": {
      "webpack": "presence",
      "webpack-cli": "presence",
      "webpack-merge": "presence",
      "@swc/core": "presence",
      "swc-loader": "presence"
