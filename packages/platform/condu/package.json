{
  "name": "condu",
  "version": "0.1.7",
  "license": "MIT",
  "sideEffects": true,
  "type": "module",
  "bin": {
    "condu": "cli.ts"
  },
  "exports": {
    ".": {
      "import": "./main.ts",
      "default": "./main.ts"
    },
    "./configure.js": {
      "import": "./configure.js",
      "default": "./configure.js"
    },
    "./defineFeature.js": {
      "import": "./defineFeature.js",
      "default": "./defineFeature.js"
    }
  },
  "dependencies": {
    "@condu/cli": "workspace:^",
    "@condu/types": "workspace:^",
    "@condu/update-specifiers": "workspace:^",
    "@condu/workspace-utils": "workspace:^",
    "@condu/schema-types": "workspace:^",
    "@pnpm/catalogs.config": "^0.1.0",
    "@pnpm/exportable-manifest": "^7.0.1",
    "@pnpm/fetch": "^8.0.4",
    "@pnpm/logger": "^5.0.0",
    "@pnpm/network.auth-header": "^3.0.1",
    "@pnpm/npm-resolver": "^21.0.2",
    "@pnpm/read-project-manifest": "^6.0.5",
    "@pnpm/types": "^12.2.0",
    "@pnpm/workspace.find-packages": "^4.0.2",
    "@pnpm/workspace.read-manifest": "^2.2.0",
    "spdx-license-list": "^6.9.0"
  },
  "devDependencies": {
    "@swc/core": "^1.10.0",
    "@types/which": "^3.0.4",
    "swc-loader": "^0.2.6",
    "webpack": "^5.97.1",
    "webpack-cli": "^5.1.4",
    "webpack-merge": "^6.0.1"
  },
  "//dependencies": "these need to inherit/include all dependencies from @condu/cli",
  "condu": {
    "initialDevelopment": true,
    "managedDependencies": {
      "webpack": "presence",
      "webpack-cli": "presence",
      "webpack-merge": "presence",
      "@swc/core": "presence",
      "swc-loader": "presence"
    }
  },
  "publishDependencies": [
    "@pnpm",
    "@condu/update-specifiers",
    "@condu/types",
    "@condu/workspace-utils",
    "@condu/schema-types",
    "spdx-license-list"
  ]
}
